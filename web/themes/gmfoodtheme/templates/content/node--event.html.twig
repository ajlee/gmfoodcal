{#
/**
 * @file
 * Template for a Node.
 */
#}
{%
  set classes = [
  'node',
  node.isPromoted() ? 'node--promoted',
  node.isSticky() ? 'node--sticky',
  not node.isPublished() ? 'node--unpublished',
  node.bundle|clean_class,
  node.bundle|clean_class ~ '--' ~ view_mode|clean_class,
]
%}
<article{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title_suffix }}
  {% block content %}
    {% block title %}{% endblock %}

    <div{{ content_attributes }}>
      {{ content }}
    </div>
  {% endblock %}
</article>

{{ content.field_more_information_link.0['#url'] }}
{{ node.field_more_information_link.0['#url'] }}

{{ kint(content.field_more_information_link.0['#url']) }}
{{ kint(node.field_more_information_link.0['#url']) }}


{{ kint(content.field_more_information_link.0) }}
{{ kint(node.field_more_information_link.0) }}
