{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.27",
        "drupal/admin_toolbar_tools": "^1.27",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/administration_language_negotiation": "^1.6",
        "drupal/allow_iframed_site": "^2.0",
        "drupal/allowed_formats": "^1.1",
        "drupal/amswap": "^2.0@beta",
        "drupal/antibot": "^1.2",
        "drupal/block_class": "^1.0",
        "drupal/block_exclude_pages": "^1.3",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/bootstrap_sass": "^1.9",
        "drupal/calendar": "1.x-dev",
        "drupal/captcha": "^1.0@beta",
        "drupal/ckeditor_bidi": "^2.1",
        "drupal/coffee": "^1.0@beta",
        "drupal/colorbox": "^1.4",
        "drupal/components": "^1.1",
        "drupal/console": "^1.9",
        "drupal/console-core": "^1.9.0",
        "drupal/console-en": "^1.9.0",
        "drupal/core": "^8.7.4",
        "drupal/ctools": "^3.2",
        "drupal/date_recur": "^1.0",
        "drupal/datetime_extras": "^1.0@alpha",
        "drupal/discourse_sso": "^1.0@beta",
        "drupal/ds": "^3.3",
        "drupal/duration_field": "^1.6",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_reference_views_select": "^1.3",
        "drupal/extlink": "^1.1",
        "drupal/field_group": "^3.0-rc1",
        "drupal/fitvids": "^1.0",
        "drupal/fullcalendar": "1.x-dev",
        "drupal/fullcalendar_view": "^2.2",
        "drupal/geolocation": "^1.11",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/imce": "^1.7",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^4.3",
        "drupal/markdown": "^1.2",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/optional_end_date": "^1.0@beta",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/radix": "^4.3",
        "drupal/radix_layouts": "^4.1",
        "drupal/redirect": "^1.4",
        "drupal/simple_gmap": "^1.4",
        "drupal/simple_user_management": "^1.2",
        "drupal/simplenews": "^1.0@beta",
        "drupal/simplify": "^1.0",
        "drupal/smtp": "^1.0@beta",
        "drupal/svg_image": "^1.9",
        "drupal/taxonomy_access_fix": "^2.6",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.3",
        "drupal/video_embed_field": "^2.1",
        "drupal/views_fieldsets": "^3.3",
        "drupal/views_ical": "1.x-dev",
        "drupal/webform": "^5.2",
        "drush/drush": "^8.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-require-dev": "^8.7.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/devel": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
                 "npm-asset",
                 "bower-asset"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/entity_browser": {
                   "Entity browser not found error": "https://www.drupal.org/files/issues/2018-07-17/entity_browser-key-2914385-6.patch"
	    },
            "drupal/fullcalendar": {
                    "FullCalendar block error on installing legend": "https://www.drupal.org/files/issues/fullcalendar-notice_undefined_index_module_in_fullcalendar_legend_preprocess_block-2854562-2.patch"
            },
            "drupal/core": {
                    "Provide default region value for layout builder": "https://www.drupal.org/files/issues/2019-06-18/3053906-11-default-region-value.patch"
            }
        }
    }
}
