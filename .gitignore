# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/profiles/contrib/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/config/sync/gmfood_discourse.settings.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore Sublime Text files
/sftp-config.json
/sftp-config.json.*

# Ignore .env files as they are personal
/.env

# Ignore contrib folder except bootstrap_sass (recommended to edit this directly)
/web/themes/contrib/*
!/web/themes/contrib/bootstrap_sass

# Ignore node_modules
/web/themes/contrib/bootstrap_sass/node_modules

# Private Config files
/config/sync/geolocation.settings.yml

